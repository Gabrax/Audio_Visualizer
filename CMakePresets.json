{
    "version": 2,
    "configurePresets": [
        {
            "name": "conan-deb",
            "hidden": true,
            "cacheVariables": {
                "CMAKE_TOOLCHAIN_FILE": "${sourceDir}/conan/deb/conan_toolchain.cmake"
            }
        },
        {
            "name": "rel",
            "hidden": true,
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Release",
                "CMAKE_EXPORT_COMPILE_COMMANDS": true
            },
            "warnings": {
                "unusedCli": false
            }
        },
        {
            "name": "deb",
            "hidden": true,
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Debug",
                "BUILD_TESTS": true,
                "CMAKE_EXPORT_COMPILE_COMMANDS": true
            }
        },
        {
            "name": "unix",
            "hidden": true,
            "generator": "Unix Makefiles",
            "cacheVariables": {
                "CMAKE_CXX_FLAGS_INIT": "$env{CMAKE_CXX_FLAGS} -Werror -Wall -Wextra -pedantic"
            }
        },
        {
            "name": "unix-shared",
            "hidden": true,
            "inherits": "unix",
            "cacheVariables": {
                "CMAKE_CXX_FLAGS_INIT": "$env{CMAKE_CXX_FLAGS_INIT} $env{CMAKE_CXX_FLAGS} -fPIC -Werror -Wall -Wextra -pedantic",
                "BUILD_SHARED_LIBS": true
            }
        },
        {
            "name": "MinGW",
            "hidden": true,
            "generator": "MinGW Makefiles",
            "environment": {
                "PATH": "C:/msys64/mingw64/bin;$penv{PATH}"
            },
            "cacheVariables": {
                "CMAKE_C_COMPILER": "gcc.exe",
                "CMAKE_CXX_COMPILER": "g++.exe",
                "CMAKE_CXX_FLAGS_INIT": "$env{CMAKE_CXX_FLAGS} -Werror -Wall -Wextra -pedantic"
            }
        },
        {
            "name": "MinGW-shared",
